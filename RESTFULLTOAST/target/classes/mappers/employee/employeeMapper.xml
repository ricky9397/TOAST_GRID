<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="login">
	
	<select id="selectLogin" resultType="employeeVO">
		SELECT ER_ID
		  FROM ER_LOGIN
		 WHERE ER_ID 	= #{id} 
		   AND ER_PW 	= #{pw}
	</select>


</mapper>